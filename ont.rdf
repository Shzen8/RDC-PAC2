        <rdfs:comment xml:lang="en">An event celebrating cultural expressions like art, dance, or traditions</rdfs:comment>
        <rdfs:label xml:lang="en">Cultural</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/turi#Event -->

    <owl:Class rdf:about="http://www.owl-ontologies.com/turi#Event">
