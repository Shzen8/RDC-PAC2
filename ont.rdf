        <rdfs:comment xml:lang="en">A road vehicle designed to carry many passengers</rdfs:comment>
        <rdfs:label xml:lang="en">Bus</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/turi#City -->

